{
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"eslint:recommended",
		"plugin:vue/vue3-recommended",
		"prettier"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "@typescript-eslint/parser",
		"ecmaVersion": 2022,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"node": true
	},
	"globals": {
		"defineModel": "readonly"
	},
	"rules": {
		"prettier/prettier": "error"
	},
	"plugins": ["@typescript-eslint", "prettier", "vue"]
}
